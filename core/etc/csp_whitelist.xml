<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="automaticffl" type="host">*.automaticffl.com</value>
                <value id="googleapis" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="googleapis_style" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="googleapis_image" type="host">*.googleapis.com</value>
                <value id="gstatic_image" type="host">*.gstatic.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="gstatic_font" type="host">*.gstatic.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="googleapis_script" type="host">*.googleapis.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
